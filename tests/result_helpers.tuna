// expect: ok
// expect: ready
// expect: error
// expect: oops

import { log, error } from "prelude"

export function main(): void {
  const a: string | error = "ready"
  switch (a) {
    case a as string: {
      log("ok")
      log(a)
    }
    case a as error: {
      log("error")
      log(a.message)
    }
  }

  const b: string | error = error("oops")
  switch (b) {
    case b as string: {
      log("ok")
      log(b)
    }
    case b as error: {
      log("error")
      log(b.message)
    }
  }
}
