// expect: Name: Alice
// expect: Age: 30
// expect: City: Tokyo
// expect: Zip: 1000001

import { log, to_string } from "prelude"

export function main(): void {
  const person = { "name": "Alice", "age": 30 }
  const { name, age } = person
  log("Name: " + name)
  log("Age: " + to_string(age))

  // 型注釈付きのdestructuring
  const person2: { city: string, zip: i64 } = { "city": "Tokyo", "zip": 1000001 }
  const { city: string, zip: i64 } = person2
  log("City: " + city)
  log("Zip: " + to_string(zip))
}
