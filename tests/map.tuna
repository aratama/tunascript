// expect: [2,4,6,8]

import { log } from "prelude"
import { stringify } from "json"
import { map, filter, reduce } from "array"

const nums: integer[] = [1, 2, 3, 4]

const genericMap: <T>(arr: T[], fn: (n: T) => T) => T[] = map

const numericMap: (arr: integer[], fn: (n: integer) => integer) => integer[] = genericMap

const doubled: integer[] = numericMap(nums, function (n: integer): integer {
  return n * 2
})

export function main(): void {
  log(stringify(doubled))
}
