{
  "name": "negitoro",
  "displayName": "Negitoro Language",
  "description": "Syntax highlighting for the Negitoro programming language",
  "version": "0.1.0",
  "publisher": "negitoro",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "negitoro",
        "aliases": [
          "Negitoro",
          "negitoro"
        ],
        "extensions": [
          ".ngtr"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "negitoro",
        "scopeName": "source.negitoro",
        "path": "./syntaxes/negitoro.tmLanguage.json"
      }
    ]
  }
}
